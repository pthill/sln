<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sln.dao.shop.write.member.MemberGradeDownLogsWriteDao">
	<resultMap id="MemberGradeDownLogsResult" type="com.sln.entity.member.MemberGradeDownLogs">
			<result property="id" column="id" />
			<result property="excuteTime" column="excute_time" />
			<result property="createTime" column="create_time" />
	</resultMap>
	
	<!-- <select id="get" parameterType="Integer" resultMap="MemberGradeDownLogsResult">
		select
		   *
		from `member_grade_down_logs`
		where `id` = #{id}
	</select> -->
	
	<select id="getByExcuteTime" resultMap="MemberGradeDownLogsResult">
		select
			*
		from `member_grade_down_logs`
		where `excute_time`= #{excuteTime}
	</select>

	<insert id="insert" parameterType="com.sln.entity.member.MemberGradeDownLogs" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into 
		`member_grade_down_logs`
		(
			`excute_time`,
			`create_time`
		)
		values
		(
			#{excuteTime},
			#{createTime}
		)
	</insert>
	
</mapper>