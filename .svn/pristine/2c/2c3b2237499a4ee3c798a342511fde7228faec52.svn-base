}

#table {
	margin-top: 20px;
}

.w200 {
	width: 200px !important;
	height: 30px;
	border-radius: 0;
	float: left;
}

input[type="text"].error,input[type="password"].error,textarea.error {
	border-color: #ED6C4F;
	box-shadow: 0 0 0 2px rgba(232, 71, 35, 0.15);
	outline: 0 none;
}

input.error,input:hover.error,input:focus.error,textarea.error,textarea.error:hover,textarea.error:focus
	{
	background-color: #FFBFBF;
	border: dotted 1px #ed6f05;
}

label.error {
	color: #333;
	line-height: 18px;
	background: #FFEEEE url(../img/joinin_pic.png) no-repeat -395px -135px;
	vertical-align: middle;
	display: inline-block;
	height: 22px;
	padding: 1px 5px 1px 20px;
	margin-left: 4px;
	border: solid 1px #FCC;
	font-weight: 400;
	margin-top: 4px;
	margin-bottom: 0px;
}

.w50 {
	width: 50px !important;
}

.w100 {
	width: 100px !important;
}

input[type="file"] {
	background-color: #FFF;
	vertical-align: middle;
	display: inline-block;
}

.joinin-concrete table.all tbody td span {
	color: #777;
	clear: both;
	margin-top: 10px;
}

.joinin-concrete table.all thead th em {
	color: #ed6f05;
