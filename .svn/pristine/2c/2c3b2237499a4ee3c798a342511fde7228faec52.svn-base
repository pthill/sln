	font-weight: 400;
}

.joinin-concrete table.all tbody td p.emphasis {
	color: #ed6f05;
	font-family: "微软雅黑";
	margin-top: 10px;
	clear: both;
}

select,.select {
	color: #777;
	background-color: #FFF;
	height: 26px;
	vertical-align: middle;
	padding: 2px;
	border: solid 1px #CCC;
