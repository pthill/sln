#newstypeTree img {
	max-width: 390px;
	max-height: 290px;
}

.leftdiv {
	width: 300px;
	float: left;
	margin-right: 1px;
}

.adv img{
	width: 100%;
	height: 200px;
}

.brandlist{
	height: 125px;
	overflow: hidden;
	margin-top: 15px;
}

.brandlist li img{
	width: 80px;
	height: 30px;
}

#editadv {
	position: absolute;
	background: #808080;
	top: 303px;
	left: 205px;
	padding: 5px;
	width: 90px;
	opacity: 0.7;
	filter: alpha(opacity = 70);
	float: right;
	font-family: 宋体;
	font-size: 14px;
	cursor: pointer;
}

#editadv:hover {
	background-color: #989898;
}

.rightdiv {
	float: left;
	width: 834px;
}

.handlerdiv p a {
	font-weight: 500;
	font-size: 35px;
	font-family: 宋体;
}

.floorclass {
	width: 1135px;
	height: 320px;
	border: 1px solid #E6D4D4;
	margin: 0 0 15px 0;
}

.fcName {
	background: #DDD none repeat scroll 0% 0%;
	padding: 5px;
	font-size: 20px;
	color: #000;
	line-height: 33px;
}

.floorclass:hover {
	border: 1px solid red;
}

.floorclass ul {
	list-style: none;
}

.rightdiv ul li {
	float: left;
	padding: 8px 14px;
	cursor: pointer;
	border: 1px solid transparent
}

.leftdiv ul li {
	float: left;
	padding: 0 10px;
}

.rightdiv li img {
	height: 120px;
	width: 178px;
}

.handlerdiv {
	position: absolute;
	background: gray;
	z-index: 9003;
	display: none;
	opacity: 0.7;
	filter: alpha(opacity = 70);
}

.handlerdiv p {
	width: 90px;
	float: left;
	height: 30px;
	font-size: 21px;
	line-height: 26px;
	padding: 15px;
}

.handlerdiv p:hover {
	background-color: #D1EEEE;
	border: 1px solid gray;
	cursor: pointer;
}

.proInfo {
	background: #D1F0D6 none repeat scroll 0% 0%;
	background-image: none;
	background-repeat: repeat;
	background-attachment: scroll;
	background-position: 0% 0%;
	background-clip: border-box;
	background-origin: padding-box;
	background-size: auto auto;
	line-height: 30px;
	position: absolute;
	text-align: center;
	font-style: normal;
	white-space: normal;
	z-index: 20;
	display: none;
	border-radius: 5px;
	font-size: 14px;
	color: #271E42;
	padding: 0 10px;
	text-align: left;
}