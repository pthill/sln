.clearfix {
    zoom: 1;
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
    clear: both;
}
a {
    color: #333333;
    text-decoration: none;
}

a:hover {
    color: #F67919;
    text-decoration: none;
}

.hidden {
    visibility: hidden;
}

.fl {
    float: left;
}

.fr {
    float: right;
}
.map-content{
    padding-bottom: 30px;
}
.form-select .mr0 {
    margin-right: 0;
}

.Cootion {
    width: 100%;
    background-color: #f5f5f5;
    padding: 50px 0px;
}

.left {
    float: left;
    width: 75%;
    height: 100%;
}

.right {
    float: right;
    width: 25%;
    height: 100%;
    padding-top: 50px;
    /* background-color: skyblue; */
    text-align: center;
}

.right h3 {
    font-size: 16px;
    margin: 15px;
    margin-bottom: 5px;
    margin-left: 60px;
    text-align: left;
}

.right .park {
    width: 58%;
    height: 30px;
    line-height: 30px;
    margin-left: 60px;
    margin-bottom: 25px;
    font-size: 14px;
    background-color: #ddd;
    cursor: pointer;
}

.right .park2 {
    width: 58%;
    height: 30px;
    line-height: 30px;
    margin-left: 60px;
    margin-bottom: 10px;
    font-size: 14px;
    background-color: #ddd;
    cursor: pointer;
}

.right .active {
    background-color: #ed6f05;
    color: #fff;
}

.m_map {
    margin-top: 30px;
    width: 889px;
    height: 685px;
    position: relative;
    background: url(../images/map/map.png) no-repeat 0 0;
}

.m_map p {
    font-size: 14px;
}

.m_map span {
    font-weight: bold;
}

.tait {
    font-size: 36px;
    font-weight: bold;
    color: #666666;
    text-align: center;
    left: 100px;
}

.tait span {
    color: #ff6d3b;
}

.tait2 {
    font-size: 36px;
    font-weight: bold;
    color: #666666;
    text-align: center;
    padding: 50px 0px;
}

.radius {
    position: absolute;
    top: -30px;
    right: -30px;
    width: 60px;
    height: 60px;
    border-radius: 50%;
    /* background-color: rgba(240, 226, 45, .4); */
    z-index: 1;
}

.radius2 {
    position: absolute;
    top: -50px;
    right: -50px;
    width: 100px;
    height: 100px;
    border-radius: 50%;
    background-color: rgba(240, 226, 45, .4);
}

.radius3 {
    position: absolute;
    top: -40px;
    right: -40px;
    width: 10px;
    height: 10px;
    z-index: 1;
}

.mp {
    position: absolute;
    cursor: pointer;
    -webkit-transition: all .2s ease-out;
    -moz-transition: all .2s ease-out;
    -ms-transition: all .2s ease-out;
    -o-transition: all .2s ease-out;
    transition: all .2s ease-out;
    z-index: 20;
}

.map-dot {
    position: relative;
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background-color: #ff3300;
    z-index: 10;
}

.hover_tu {
    background: url(../images/map/fi2.png) no-repeat 4px 20px;
}

.mp .mito {
    position: absolute;
    left: 0px;
    bottom: 0px;
    font-size: 12px;
    color: #666666;
}

.mp .find_mi1 {
    left: -12px;
    width: 55px;
}

.mp .find_mi2 {
    left: -5px;
    width: 55px;
}

.feng {
    position: absolute;
    display: none;
    width: 230px;
    left: -90px;
    top: -224px;
    background-color: rgba(24, 24, 23, .7);
    z-index: 9999;
}

.feng .sang {
    position: absolute;
    left: 108px;
    bottom: -8px;
    background: url(../images/map/vf2.png) no-repeat 0 0;
    width: 14px;
    height: 8px;
}

.feng img {
    width: 100%;
    height: 130px;
    padding: 5px;
}

.feng .bottom {
    padding-left: 10px;
    padding-right: 10px;
}

.feng p {
    color: #FFF;
