.bg-color {
  background: #EF3030;
}
.bg-color > div > span ,
.bg-color > div {
  color: #fff;
}
/*S 导航*/
.nav_area {
    position: relative;
    z-index: 100;
    width: 100%;
    /*max-width: 640px;*/
}
.nav_area>div.nav_top_area {
    position: relative;
    overflow: hidden;
    height: 4.5rem;
    border-bottom: 1px solid #cbcbcb;
}
.tab_nav {
    background-color: #fff;
    /*border-bottom: 1px solid #cbcbcb;*/
    box-sizing: border-box;
    margin-right: 4.5rem;
    overflow: hidden;
}
.tab_nav ul {
    overflow: hidden;
    position: relative;
    left:0;
}
.tab_nav ul li {
    text-align: center;
    font-size: 1.2rem;
    height: 100%;
    line-height: 4.2rem;
    float: left;
    padding: 0 0.6rem;
}
.tab_nav ul li a {
    display: inline-block;
    margin: 0 .3rem;
    height: 100%;
}
.tab_nav ul li a.active {
    border-bottom: .2rem solid #ef3030;
    color: #ef3030;
    box-sizing: border-box;
}
.show_more_list {
    position: absolute;
    display: none;
    top: 4rem;
    background: #fff;
    width: 100%;
    max-width: 640px;
    height: auto;
    z-index: 9999;
    box-shadow: 0px 5px 14px #d6d6d6;
}
.show_more_list li {
    float: left;
    width: 25%;
    text-align: center;
    padding: 1rem 0;
}
.show_more_list li a.active {
    color: #ef3030;
}
.tog_more_list {
  display: block;
}

.show_more {
    position: absolute;
    z-index: 101;
    right: 0;
    top: 0;
    width: 4.4rem;
    height: 100%;
    line-height: 4.4rem;
    background: url("../img/bor-img_03.png") left center no-repeat #fff;
    background-size: 0.25rem 4rem;
    text-align: center;
    color: #7f7f7f;
}
/*E 导航*/

/*S 商品*/
.i-list-box {
    background: #fff;
}
.i-list-box h2 {
    font-size: 14px;
    padding: 10px 0px;
}
.i-list-ul li {
    float: left;
    width: 50%;
    margin-bottom: 10px;
    padding: 0 5px;
    height: 245px;
}
.i-list-img {
    margin-bottom: 0px;
    text-align: center;
    /* padding-bottom: 5px; */
    padding: 5px 0;
}
.product_name {
    font-size: 14px;
    margin-bottom: 0px;
    line-height: 20px;
    height: 38px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    color: #252525;
}
.i-list-price {
    color: #f15353;
    border-top: solid 1px #dedede;
    padding: 5px;
    text-align: left;
}
.group_section h2:before {
    content: '';
    height: 1.7rem;
    border-left: 0.25rem solid #ef3030;
    margin-right: 10px;
}
.have_buy {
    margin-left: .5rem;
    color: #ccc;
}
.integral_buy {
    color: #ccc;
}

.member-center-box {
    width: 100%;
    padding: 10px;
    
}
.member-center-box .member-center-info {
    width: 100%;
    height: 100%;
    /**/
}
.member-center-box .member-center-info .fl-box {
    margin-right: 4%;
    text-align: center;
}
.member-center-box .member-center-info .fl-box, 
.member-center-box .member-center-info .fr-box {
    width: 100%;
    height: 100%;
    border: 1px solid #e8e8e8;
    float: left;
    background: #f9f9f9;
    padding: 0 10px;
    height: 110px;
}
.yes-loginvip {
    margin-top: 26px;
    font-size: 1.2rem;
}
.no-loginvip {
    display: inline-block;
    padding: 5px 20px;
    background: #ff8a00;
    margin-top: 40px;
    color: #fff;
    border-radius: 6px;
}
.vip-name {
    color: #3e3e3e;
    font-size: 1.4rem;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-bottom: 0.4rem;
}
.mem-grade {
    color: #ff8a00;
}
.vip-lvbg {
    width: 100%;
    height: 6px;
    background: #DEDEDE;
    border-radius: 2px;
    margin: 10px 0;
}
.vip-nowlv {
    height: 6px;
    background: #FF7900;
    border-radius: 2px;
}
.member-center-box .member-center-info .fr-box {
    font-size: 1.2rem;
    text-align: center;
}
.fr-box .p-integral {
    padding-top: 1.6rem;
}
.fr-box .p-one {
    line-height: 2.0rem;
    margin-bottom: 0.5rem;
}
.vip-signbtn {
    display: inline-block;
    color: #fff;
    background-color: #ff8a00;
    border: 1px solid #ff8a00;
    cursor: pointer;
    width: 100%;
    padding: 0.4rem;
    border-radius: 5px;
}
.vip-signbtn span {
    padding: 0.3rem;
}
.mbr-gd {
    margin-top: 12px;
}
.mbr-gd .gd-tit {
    text-align: right;
}
.mbr-gd .gd-ul {
    display: none;
}
.mbr-gd .gd-ul.showUl {
    display: block;
}
.mbr-gd .gd-ul li a {
    width: 100%;
    height: 100%;
    display: inline-block;
    border-radius: 4px;
    padding: 0.5rem 0;
    color: #000;
}
.mbr-gd .gd-ul li a span {
    padding-left: 10px;
    display: inline-block;
    line-height: 35px;
}

.container {
    padding:0px;
    margin: 0px;
}
.slidebar.slidebar-width {
    width: 70%;
}
.pullwidth {
    -webkit-transform: translate3d(-70%, 0px, 0px);
    -moz-transform: translate3d(-70%, 0px, 0px);
    transform: translate3d(-70%, 0px, 0px);
    -moz-box-shadow: 3px -3px 6px rgba(0, 0, 0, 0.8);
    -o-box-shadow: 3px -3px 6px rgba(0, 0, 0, 0.8);
    -webkit-box-shadow: 3px -3px 6px rgba(0, 0, 0, 0.8);
    box-shadow: 3px -3px 6px rgba(0, 0, 0, 0.8);
}
.menu-2-ul.menu-2-ul-m {
    display: block;
    padding-left: 40px;
}
.i-list-img .item-price-spread {
	width: 77px;
  height: 77px;
  position: absolute;
  top: 0px;
  right: -10px;
}
.item-spread {
    width: 42px;
    position: absolute;
    top: 14px;
    right: 19px;
    color: rgb(255, 255, 255);
    font-size: 14px;
    text-align: center;
    line-height: 16px;
    font-family: 'Microsoft YaHei UI', 'Microsoft YaHei UI Light', 微软雅黑, 宋体, Helvetica-Black-SemiBold, Tahoma, Arial, sans-serif;
}
.item-spread-money {
    display: block;
    width: 42px;
