.nav {
    position: relative;
    width: 100%;
    z-index: 110;
    /* background-color: #fff; */
}

.nav .head-v3 {
    position: relative;
    z-index: 100;
    width: 100%;
    height: 43px
}

.nav .head-v3 .navigation-inner {
    position: relative;
    height: 50px
}

.nav .navigation-up {
    height: 43px;
    line-height: 43px;
    background: #fff
}

.nav .navigation-up .navigation-v3 li {
    float: left;
    color: #666;
    text-align: center;
    padding: 0;
    height: 43px;
    line-height: 43px;
    cursor: pointer;
}

.nav .navigation-up .navigation-v3 li a i {
    display: inline-block;
    width: 20px;
    height: 20px;
    vertical-align: middle;
    margin-left: 12px;
    margin-top: 10px;
    background-image: url(../images/home/icon/jian.png);
    background-repeat: no-repeat;
    background-position: -5px -8px
}


/* .nav .navigation-up .navigation-v3 li img {
    margin-top: 15px
} */

.nav .navigation-up .navigation-v3 li:hover a i {
    background-position: -5px -28px
}

.nav .navigation-up .navigation-v3 .nav-up-selected-inpage {
    background: #ed6c00;
}

.nav .navigation-up .navigation-v3 li h2 {
    position: relative;
    font-weight: 400;
    padding: 0;
    margin: 0;
    line-height: 43px;
}
.nav .navigation-up .navigation-v3 li.num1 h2:after{
    content: "";
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -10px;
    display: block;
    width: 0;
    height: 20px;
    border-right: 1px solid #ccc;
}
.nav .navigation-up .navigation-v3 li h2 a {
    padding: 0 26px;
    color: #000;
    display: block;
    line-height: 43px;
    font-size: 14px;
    text-decoration: none;
}
.nav .navigation-up .navigation-v3 .nav-up-selected h2:after{
    content: "";
    display: none;
}
.nav .navigation-up .navigation-v3 .nav-up-selected h2 a{
    color: #ed6c00;
}
.nav .navigation-up .navigation-v3 .nav-up-selected a,
.nav .navigation-up .navigation-v3 .last-li a {
    border-right: 0px;
}

.nav .navigation-up .navigation-v3 li h2 a img {
    margin-left: 5px
}

.nav .navigation-up .navigation-v3 li.num1 a,
.nav .navigation-up .navigation-v3 li.num2 a,
.nav .navigation-up .navigation-v3 li.num3 a,
.nav .navigation-up .navigation-v3 li.num4 a,
.nav .navigation-up .navigation-v3 li.num5 a,
.nav .navigation-up .navigation-v3 li.num6 a {
    padding: 0 0 0 15px
}

.nav .navigation-up .navigation-v3 li.navigation-active {
    background-color: #ed6c00
}

.nav .navigation-up .navigation-v3 li.navigation-active h2 a {
    color: #fff;
}
.nav .navigation-up .navigation-v3 li.navigation-active h2:after{
    content: "";
    border-right: 1px solid #ed6c00;
}
.nav .navigation-down {
    position: relative;
    width: 100%;
    z-index: 9999;
    border-bottom: 2px solid #ed6c00
}

.nav .navigation-down .nav-down-menu {
    position: absolute;
    left: 0;
    top: 3px;
    width: 100%;
    height: 40px;
    line-height: 40px;
    margin: 0 auto;
    background-color: #fff;
    box-shadow: 2px 3px 4px rgba(0, 0, 0, .15);
    z-index: 1000;
}

.nav .navigation-down .nav-down-menu .navigation-down-inner {
    width: 1200px;
    margin: 0 auto;
    position: relative
}

