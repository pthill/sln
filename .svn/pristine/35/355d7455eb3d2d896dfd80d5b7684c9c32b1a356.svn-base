.slide-wrap .bd {
    position: relative;
    z-index: 0
}

.slide-wrap .bd ul li {
    height: 420px;
}
.slide-wrap .bd ul li a{
    display: block;
}
.slide-wrap .bd ul li .slide-line {
    text-align: center;
}

.slide-wrap .bd ul li .mobile dl {
    width: 100%;
}

.slide-wrap .bd ul li .mobile p {
    margin-bottom: 12px;
}

.slide-wrap .bd ul li .mobile-bottom p {
    margin-bottom: 15px;
}

.slide-wrap .bd ul li .mobile dl dd {
    display: inline-block;
    width: 32%;
}

.slide-wrap .bd ul li .mobile-bottom dl dd {
    width: 100%;
}

.slide-wrap .bd ul li .mobile-bottom dl dd img {
    display: inline-block;
    width: 32%;
}


/* .slide-wrap .bd ul li img {
    width: 32%;
    vertical-align: top
} */

.slide-wrap .hd {
    position: relative;
    z-index: 1;
    margin-top: -30px;
    height: 30px;
    line-height: 30px;
    text-align: center;
}

.slide-wrap .hd ul {
    position: absolute;
    left: 160px;
    bottom: 10px;
}

.slide-wrap .hd ul li {
    display: inline-block;
    zoom: 1;
    width: 10px;
    height: 10px;
    margin: 5px;
    border-radius: 50%;
    background-color: #666;
    cursor: pointer;
    overflow: hidden
}

.slide-wrap .hd ul li.on {
    background-color: #f30
}

.slide-wrap .arrowbtn {
    position: absolute;
    top: -50px;
    display: block;
    width: 20px;
    height: 49px;
    z-index: 100
}

.slide-wrap .prev {
    left: 11px;
    top: 165px;
}

.slide-wrap .next {
    right: 11px;
    top: 165px;
}
.list-wrap{
    height: 420px;
    border: 1px solid #ccc;
}
.list-wrap li{
    height: 200px;
    margin-top: 10px;
}
.main-container{
    padding-bottom: 30px;
