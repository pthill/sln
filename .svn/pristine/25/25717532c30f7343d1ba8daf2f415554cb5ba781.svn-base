<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sln.dao.shop.write.member.MemberGradeIntegralLogsWriteDao">
	<resultMap id="MemberGradeIntegralLogsResult" type="com.sln.entity.member.MemberGradeIntegralLogs">
			<result property="id" column="id" />
			<result property="memberId" column="member_id" />
			<result property="memberName" column="member_name" />
			<result property="value" column="value" />
			<result property="createTime" column="create_time" />
			<result property="optType" column="opt_type" />
			<result property="optDes" column="opt_des" />
			<result property="refCode" column="ref_code" />
			<result property="type" column="type" />
			<result property="msiId" column="msi_id" />
	</resultMap>
	<select id="get" parameterType="Integer" resultMap="MemberGradeIntegralLogsResult">
		select
		   *
		from `member_grade_integral_logs`
		where `id` = #{id}
	</select>
	
	<update id="update" parameterType="com.sln.entity.member.MemberGradeIntegralLogs">
        update `member_grade_integral_logs` 
    	<set>
			<if test="memberId != null">`member_id`= #{memberId},</if>
			<if test="memberName != null">`member_name`= #{memberName},</if>
			<if test="value != null">`value`= #{value},</if>
			<if test="optType != null">`opt_type`= #{optType},</if>
			<if test="optDes != null">`opt_des`= #{optDes},</if>
			<if test="refCode != null">`ref_code`= #{refCode},</if>
			<if test="type != null">`type`= #{type}</if>,
			<if test="msiId != null">`msi_id`= #{msiId}</if>
	    </set>
        where `id` = #{id}
	</update>
	
	<insert id="save" parameterType="com.sln.entity.member.MemberGradeIntegralLogs" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into 
		`member_grade_integral_logs`
		(
			`member_id`,
			`member_name`,
			`value`,
			`create_time`,
			`opt_type`,
			`opt_des`,
			`ref_code`,
			`type`,
			`msi_id`
		)
		values
		(
			#{memberId},
			#{memberName},
			#{value},
			now(),
			#{optType},
			#{optDes},
			#{refCode},
			#{type},
			#{msiId}
		)
	</insert>
	
</mapper>