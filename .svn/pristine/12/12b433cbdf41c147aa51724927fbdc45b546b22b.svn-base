<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sln.dao.shop.read.seller.SellerUserLoginLogReadDao">
	<resultMap id="SellerUserLoginLogResult" type="com.sln.entity.seller.SellerUserLoginLog">
			<result property="id" column="id" />
			<result property="userId" column="user_id" />
			<result property="userName" column="user_name" />
			<result property="loginIp" column="login_ip" />
			<result property="createTime" column="create_time" />
	</resultMap>
	
	<select id="get" parameterType="Integer" resultMap="SellerUserLoginLogResult">
		select
		   *
		from `seller_user_login_log`
		where `id` = #{id}
	</select>
	
</mapper>