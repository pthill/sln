.wrapper_main-wd .mc {
	line-height: 20px;
	overflow: hidden;
}

.stepflex {
	border-top: 5px solid #ccc;
	text-align: center;
	margin: 30px auto;
	display: block;
	width: 570px;
}

.stepflex dl {
	float: left;
	position: relative;
	width: 190px;
	top: -5px;
	border-top: 5px solid #ccc;
}

dl.doing {
	border-top-color: #7abd54;
}

.s-num {
	position: relative;
	margin: -15px auto 0;
	color: #fff;
	font-weight: 700;
	width: 23px;
	height: 23px;
	background-image: url(../img/step2013.png);
	line-height: 23px;
}

.doing .s-num {
	background-position: -23px 0;
}

.doing b, .doing s, .first s, .last b {
	width: 5px;
	height: 5px;
	position: absolute;
	line-height: 0;
	font-size: 0;
	top: -5px;
	background-image: url(../img/step2013.png);
	overflow: hidden;
}

.stepflex s {
	left: 0;
}

.first s {
	background-position: 0 -24px;
	background-color: #fff;
}

.doing s {
	background-position: -23px -24px;
}

.s-text {
	color: #ccc;
}

.doing .s-text {
	color: #7abd54;
}

.last .s-num {
	background-position: -68px 0;
}

.stepflex b {
	right: 0;
}

.last b {
	background-position: -5px -24px;
	background-color: #fff;
}

.doing.last .s-num {
	background-position: -91px 0;
}

.doing.last b {
	background-color: #fff;
}

.doing b {
	background-position: -28px -24px;
	background-color: #ccc;
}

.form-boxzh {
	width: 480px;
	margin: 0px auto;
}

.form-boxzh .user-zh {
	width: 200px;
	padding: 5px 5px;
	margin-bottom: 30px;
}

.form-boxzh .item-user {
	position: relative;
}

.form-boxzh .item-user .err-box {
	position: absolute;
	top: 36px;
	left: 110px;
	display: none;
}

.form-boxzh .item-user .btn-ip {
	margin-left: 110px;
	padding: 0px 12px;
}

.form-boxzh .item-user .ip-dx {
	height: 34px;
	line-height: 34px;
	background: #F6F6F6;
	border: 1px solid #e0e0e0;
	padding: 0 5px;
}

.form-boxzh .item-user span.w108 {
	display: inline-block;
	width: 108px;
	line-height: 34px;
	text-align: right;
}

.form-boxzh .item-user .tel-yz {
	margin-right: 10px;
	margin-bottom: 20px;
	font-size: 14px;
}