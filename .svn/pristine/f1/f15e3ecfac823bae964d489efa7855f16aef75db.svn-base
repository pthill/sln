	<insert id="insert" parameterType="com.sln.entity.coupon.LogCoupon" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
		insert into 
		`log_coupon`
		(
			`opt_type`,
			`opt_user_id`,
			`opt_user_name`,
			`opt_time`,
			`coupon_id`,
			`seller_id`,
			`coupon_name`,
			`prefix`,
			`coupon_value`,
			`min_amount`,
			`send_start_time`,
			`send_end_time`,
			`use_start_time`,
			`use_end_time`,
			`person_limit_num`,
			`total_limit_num`,
			`received_num`,
			`type`,
			`channel`,
			`status`,
			`audit_opinion`,
			`remark`,
			`audit_user_id`,
			`audit_user_name`,
			`audit_time`,
			`create_user_id`,
			`create_user_name`,
			`create_time`,
			`update_user_id`,
			`update_user_name`,
			`update_time`
		) select 
			#{optType},
			#{optUserId},
			#{optUserName},
			now(),
			`id`,
			`seller_id`,
			`coupon_name`,
			`prefix`,
			`coupon_value`,
			`min_amount`,
			`send_start_time`,
			`send_end_time`,
			`use_start_time`,
			`use_end_time`,
			`person_limit_num`,
			`total_limit_num`,
			`received_num`,
			`type`,
			`channel`,
			`status`,
			`audit_opinion`,
			`remark`,
			`audit_user_id`,
			`audit_user_name`,
			`audit_time`,
			`create_user_id`,
			`create_user_name`,
			`create_time`,
			`update_user_id`,
			`update_user_name`,
			`update_time`
		from `coupon` coupon where coupon.`id` = #{couponId}
	</insert>
	
